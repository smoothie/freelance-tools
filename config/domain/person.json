{
    "id": {
        "type": "uuid"
    },
    "name": {
        "type": "computed(person.firstName + person.lastName)"
    },
    "first_name": {
        "type": "varchar",
        "length": 255
    },
    "last_name": {
        "type": "varchar",
        "length": 255
    },
    "company": {
        "type": "json",
        "domain": "Organization"
    },
    "address": {
        "type": "json",
        "domain": "Address"
    }
}
